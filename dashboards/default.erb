<% content_for :title do %>Dashboard<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
        <div data-id="clock" data-view="Clock" data-title="Clock" style="background-color:#61a74a;"></div>
    </li>
    <li data-row="2" data-col="3" data-sizex="2" data-sizey="1">
        <div data-id="google_calendar_personal" data-view="GoogleCalendar" style="background-color:#4183C5;"></div>
        <i class="icon-calendar icon-background"></i>
    </li>
    <li data-row="1" data-col="2" data-sizex="2" data-sizey="1">
        <div data-id="google_calendar_work" data-view="GoogleCalendar" style="background-color:#EB5424;"></div>
        <i class="icon-calendar icon-background"></i>
    </li>
    <li data-row="2" data-col="1" data-sizex="1" data-sizey="1">
        <div data-view="PullRequests" data-id="gh_prs"></div>
    </li>
    <li data-row="2" data-col="3" data-sizex="1" data-sizey="1">
        <div data-id="myiframe" data-view="Iframe"></div>
    </li>
  </ul>
</div>
